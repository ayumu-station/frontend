{
	"name": "station",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "dotenv -e .env.dev -- vite dev",
		"dev:prod": "dotenv -e .env.prod -- vite dev",
		"dev:host": "dotenv -e .env.dev -- vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prisma": "dotenv -e .env.dev -- npx prisma",
		"prisma:db push": "dotenv -e .env.dev -- npx prisma db push",
		"prisma:db push prod": "dotenv -e .env.prod -- npx prisma db push",
		"prisma:generate": "dotenv -e .env.dev -- npx prisma generate",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"devDependencies": {
		"@playwright/test": "^1.30.0",
		"@storybook/addon-essentials": "7.0.0-beta.38",
		"@storybook/addon-interactions": "7.0.0-beta.38",
		"@storybook/addon-links": "7.0.0-beta.38",
		"@storybook/blocks": "7.0.0-beta.38",
		"@storybook/svelte": "7.0.0-beta.38",
		"@storybook/sveltekit": "7.0.0-beta.38",
		"@storybook/testing-library": "0.0.14-next.1",
		"@sveltejs/adapter-auto": "latest",
		"@sveltejs/adapter-netlify": "^2.0.5",
		"@sveltejs/adapter-node": "^1.2.0",
		"@sveltejs/kit": "latest",
		"@types/pngjs": "^6.0.1",
		"@types/regenerate": "^1.4.1",
		"@types/youtube-player": "^5.5.6",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.13",
		"concurrently": "^7.6.0",
		"dotenv": "^16.0.3",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.19",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"prisma": "^4.8.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"sass": "^1.57.1",
		"storybook": "7.0.0-beta.38",
		"svelte": "^3.54.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.267.0",
		"@lexical/plain-text": "^0.7.6",
		"@lucia-auth/adapter-kysely": "^0.5.0",
		"@lucia-auth/sveltekit": "^0.6.3",
		"@planetscale/database": "^1.5.0",
		"buffer": "^6.0.3",
		"devalue": "^4.2.0",
		"jimp": "^0.22.4",
		"kysely": "^0.23.4",
		"kysely-planetscale": "^1.3.0",
		"lexical": "^0.7.6",
		"lucia-auth": "^0.6.0",
		"nanoid": "^4.0.0",
		"overlayscrollbars": "^2.0.3",
		"overlayscrollbars-svelte": "^0.4.0",
		"pngjs": "^7.0.0",
		"rand-seed": "^1.0.2",
		"regenerate": "^1.4.2",
		"regenerate-unicode-properties": "^10.1.0",
		"sql-template-tag": "^5.0.3",
		"youtube-player": "^5.5.2"
	}
}
